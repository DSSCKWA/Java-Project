                                    <columns>
                                        <TableColumn fx:id="tcName" maxWidth="300.0" minWidth="100.0"
                                                     prefWidth="149.5999665260315" text="Name"/>
                                        <TableColumn fx:id="tcCity" maxWidth="300.0" minWidth="100.0"
                                                     prefWidth="241.60003662109375" text="City"/>
                                        <TableColumn fx:id="tcAddress" maxWidth="300.0" minWidth="100.0"
                                                     prefWidth="262.3999938964844" text="Address"/>
                                    </columns>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
    </children>
