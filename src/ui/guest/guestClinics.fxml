<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox prefHeight="526.0" prefWidth="889.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="src.ui.guest.GuestClinicsController">
    <children>
        <AnchorPane prefHeight="526.0" prefWidth="202.0">
            <children>
                <VBox prefHeight="526.0" prefWidth="182.0" style="-fx-background-color: #A41F03;">
                    <children>
                        <ImageView fitHeight="196.0" fitWidth="209.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../logo/logo.png"/>
                            </image>
                        </ImageView>
                        <Button alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false"
                                onAction="#btnStartClicked" prefHeight="50.0" prefWidth="226.0"
                                stylesheets="@../css/adminStyling.css" text="Start" textFill="WHITE">
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <padding>
                                <Insets left="50.0"/>
                            </padding>
                            <font>
                                <Font name="Arial Bold" size="14.0"/>
                            </font>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Button>
                        <Button alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false"
                                onAction="#btnClinicsClicked" prefHeight="50.0" prefWidth="226.0"
                                stylesheets="@../css/adminStyling.css" text="Clinics" textFill="WHITE">
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <padding>
                                <Insets left="50.0"/>
                            </padding>
                            <font>
                                <Font name="Arial Bold" size="14.0"/>
                            </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false"
                                onAction="#btnDoctorsClicked" prefHeight="50.0" prefWidth="224.0"
                                stylesheets="@../css/adminStyling.css" text="Doctors" textFill="WHITE">
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <padding>
                                <Insets left="50.0"/>
                            </padding>
                            <font>
                                <Font name="Arial Bold" size="14.0"/>
                            </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false"
                                onAction="#btnLogInClicked" prefHeight="50.0" prefWidth="222.0"
                                stylesheets="@../css/adminStyling.css" text="Log In" textFill="WHITE">
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <padding>
                                <Insets left="50.0"/>
                            </padding>
                            <font>
                                <Font name="Arial Bold" size="14.0"/>
                            </font>
                        </Button>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="526.0" prefWidth="686.0">
            <children>
                <VBox prefHeight="526.0" prefWidth="686.0">
                    <children>
                        <AnchorPane prefHeight="262.0" prefWidth="686.0">
                            <children>
                                <Text layoutX="24.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="Search Engine">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Text>
                                <Text layoutX="24.0" layoutY="93.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="Name:"/>
                                <Text layoutX="24.0" layoutY="132.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="City:"/>
                                <TextField fx:id="tfName" layoutX="108.0" layoutY="76.0" onAction="#tfNameChanged"/>
                                <TextField fx:id="tfCity" layoutX="109.0" layoutY="115.0" onAction="#tfCityChanged"/>
                            </children>
                        </AnchorPane>
                        <AnchorPane prefHeight="402.0" prefWidth="686.0">
                            <children>
                                <TableView fx:id="tvTable" layoutX="7.0" layoutY="8.0" prefHeight="322.0"
